{
  "name": "angular-prime-prediccion",
  "version": "0.0.0",
  "description": "Aplicación de predicción con Angular y PrimeNG",
  "author": "Tu Nombre <tuemail@ejemplo.com>",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "ng:serve": "ng serve",
    "build": "ng build",
    "build:electron": "ng build --configuration production && node fix-base.js",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "electron": "electron .",
    "electron:dev": "concurrently \"npm run ng:serve\" \"wait-on http://localhost:4200 && cross-env NODE_ENV=development electron .\"",
    "electron:build": "npm run build:electron && node build-electron.js",
    "electron:build-win": "npm run build:electron && node build-electron.js",
    "electron:build-mac": "npm run build:electron && electron-builder --mac",
    "electron:build-linux": "npm run build:electron && electron-builder --linux"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "main": "main.js",
  "private": true,
  "dependencies": {
    "@angular/common": "^20.3.0",
    "@angular/compiler": "^20.3.0",
    "@angular/core": "^20.3.0",
    "@angular/forms": "^20.3.0",
    "@angular/platform-browser": "^20.3.0",
    "@angular/router": "^20.3.0",
    "@primeuix/themes": "^1.2.5",
    "@tailwindcss/postcss": "^4.1.14",
    "chart.js": "^4.5.1",
    "postcss": "^8.5.6",
    "primeicons": "^7.0.0",
    "primeng": "^20.2.0",
    "rxjs": "~7.8.0",
    "tailwindcss": "^4.1.14",
    "tailwindcss-primeui": "^0.6.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.3.4",
    "@angular/cli": "^20.3.4",
    "@angular/compiler-cli": "^20.3.0",
    "@types/jasmine": "~5.1.0",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^38.4.0",
    "electron-builder": "^26.0.12",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.9.2",
    "wait-on": "^9.0.1"
  }
}
